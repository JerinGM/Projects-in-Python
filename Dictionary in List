travel_log = [
{
  "country": "France",
  "visits": 12,
  "cities": ["Paris", "Lille", "Dijon"],
},
{
  "country": "Germany",
  "visits": 5,
  "cities": ["Berlin", "Hamburg", "Stuttgart"],
},
]

def add_new_country(country, noOfVisits, ncities):
    newList = {}
    newList["country"] = country
    newList["visits"] = noOfVisits
    newList["cities"] = ncities
    travel_log.append(newList)

add_new_country("Russia", 2, ["Moscow", "Saint Petersburg"])
print(travel_log)
