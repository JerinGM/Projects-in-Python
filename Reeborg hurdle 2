def turn_right():
    turn_left()
    turn_left()
    turn_left()
def jump():
    move()
    turn_left()
    move()
    turn_right()
    move()
    turn_right()
    move()
    turn_left()
    
numOfHurdle = 6    
while numOfHurdle > 0:
    jump()
    numOfHurdle -=1
    if at_goal():
        done()

